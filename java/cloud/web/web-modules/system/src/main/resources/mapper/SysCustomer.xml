<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.toesbieya.jxc.system.mapper.SysCustomerMapper">
    <select id="getLimitRegion" resultType="cn.toesbieya.jxc.system.model.vo.RegionValueResult">
        select a.id,
               count(0) value
        from sys_region a
                     join sys_customer b on a.id = b.region
        group by a.id
    </select>
</mapper>